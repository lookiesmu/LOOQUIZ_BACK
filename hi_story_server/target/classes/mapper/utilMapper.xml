<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="util">
	
	<select id= "corpList" resultType="List">
		select sname from storeList where bid=#{bid}
	</select>

	<select id="userBadge" resultType="List">
    	select bid from userBadge where uid=#{uid}
    </select>

	<select id="badge" resultType="udto">
    	select * from badge where bid=#{bid}
    </select>

</mapper>
